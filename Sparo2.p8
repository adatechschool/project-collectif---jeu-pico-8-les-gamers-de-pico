-- player
function create_player()
	p = {
		x=003,
		y=001,
		ox=0,
		oy=0,
		start_ox=0,
		start_oy=0,
		anim_t=0,
		sprite=081,
		box=0,
		spear=0,
		shield=0,
		hearts=0
	}
end

-- draw inventaire
function i_draw()
	camera()
	cls()
	map (0,052,0,0,16,16)
	pos= 0
		for item in all(inventory) do
		spr (item,(pos%7)*16+8,flr(pos/7)*16+48,2,2)
		pos += 1
		end
	spr (110,(select%7)*16+8,flr(select/7)*16+48,2,2)
end

function i_update()
	if (btnp(⬅️)) select += (select%7 == 0 and 6 or -1)
	if (btnp(➡️)) select += (select%7 == 6 and -6 or 1)
	if (btnp(⬇️)) select += (select< 7  and 14  or -7)
	if (btnp(⬆️)) select += (select >=14 and -14 or 7)

-- utilisation de la potion
-- btn(4) = touche c	
	if (btnp (4))	and (inventory[select+1] == 096) 
	then
		sfx(8)
		p.hearts += 3 
		del(inventory,inventory[select+1])
	end 
	
-- sortir de l'inventaire
-- btn 5 = touche ❎	
	if (btnp (5)) then
		_update = g_update
		_draw = g_draw
	end
end

--ouvrir l'inventaire
function open_inventory()
		sfx(14)
	_draw = i_draw
	_update = i_update
end

function check_mob(x,y)
	for m in all (mob) do
		if (m.x==x and m.y==y) return true
	end
		return false 
end

function player_movement()
	newx=p.x
	newy=p.y
	
	if p.anim_t==0 then
		newox=0
		newoy=0
	
	if btn(➡️) then
		newx+=1
		newox=-8
		p.flip=false
	end
	if btn(⬅️) then
		newx-=1
		newox=8
		p.flip=true
	end
	if btn(⬇️) then
		newy+=1
		newoy=-8
	end
	if btn(⬆️) then
		newy-=1
		newoy=8
	end
	if btnp(❎) then
		attack()
	end
	if (btn(4)) then 
	 open_inventory()  
	end
end

	interact(newx,newy)
	
-- limite map
	if not(check_flag(0,newx,newy) or 
	check_mob(newx,newy))
	and (p.x!=newx or p.y!=newy) then
		p.x=newx
		p.y=newy
		p.x=mid(0,newx,127)
		p.y=mid(0,newy,63)
	
		p.start_ox=newox
		p.start_oy=newoy
		p.anim_t=1
	end

-- animation
	p.anim_t=max(p.anim_t-0.125,0)
	p.ox=p.start_ox*p.anim_t
	p.oy=p.start_oy*p.anim_t
	
	if p.anim_t>0.5 then 
		p.sprite=82
		sfx(6)
	else
		p.sprite=81
	end
end

-- interaction ouverture de coffre
function interact(x,y)
	if check_flag(1,x,y) then
		open_box(x,y)
		sfx(0)
	end

-- interaction & animation
	if x==3 and  y==3 then
	create_msg("welcome!","you've arrived to elsaland!\nenjoy your stay")
	sfx(13)
	end
	
	if x==2 and y==7 then
	create_msg("go away!","you shall not pass\ngo somewhere else")
	sfx(11)
	end
	
	if x==39 and y==01 then
	create_msg("road to elsaville","just a little further\nelsa is waiting")
	sfx(13)
	end
	
	if x==9 and y==2 then
	create_msg("magic chest","sorry, there's nothing here\nsomeone already came")
	end
	
	if x==49 and y==03 then
	create_msg("elsaville","help!our village is\nattacked by rabbits!")
	sfx(13)
	end
	
	if x==7 and y==7 and not killed_rabbit
	then	create_msg("evil rabbit","press ❎ to fight me\nif you dare!")
	killed_rabbit=true
	sfx(12)
	end
	
	if x==53 and y==09 then
	create_msg("elso","omg thank you so much!\ni thought that was it for me!")
	sfx(13)
	end
	
	if x==59 and y==11 then
	create_msg("elsa","congrats! you fought bravely\nand finished the game")
	music(0)
	end
	
	if check_flag(2,x,y) then
	pick_up_hearts(x,y)
	add(inventory,096)
	sfx(8)	
	end	
end

function draw_player()
	spr(p.sprite,p.x*8+p.ox,p.y*8+p.oy,1,1,p.flip)
end
