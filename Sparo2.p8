-- gameplay
-- check si mob
function get_mob(x,y)
	for m in all(mob) do
		if (m.x==x and m.y==y) return m
	end
		return false
end

-- supprimer mob
function make_anim(x,y,anim)
end

function attack()
-- 4 pos verifier mob alentours
	local pos={}
		pos[1] = {x=1,y=0}
		pos[2] = {x=-1,y=0}
		pos[3] = {x=0,y=1}
		pos[4] = {x=0,y=-1}
		
		for newpos in all(pos) do
  -- c=cible
		c=get_mob(p.x+newpos.x,p.y+newpos.y)
		if (c != false) then
		c.hp -=1
		sfx(7) 	
  -- supprimer mob quand pv=0
		make_anim(c.x,c.y,p.attack)
		if c.hp<=0 then del (mob, c)
		end
		end
		end
end
