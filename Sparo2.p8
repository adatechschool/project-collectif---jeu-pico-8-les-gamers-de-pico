-- map
function draw_map()
	map(0,0,0,0,128,64)	
end

-- collisions
function check_flag(flag,x,y)
	local sprite=mget(x,y)
	return fget(sprite,flag)
end

-- cam par section
function update_camera ()
	local camx=flr(p.x/16)*16
	local  camy=flr(p.y/16)*16
	camera(camx*8, camy*8)
end

-- animation
function next_tile(x,y)
	sprite=mget(x,y)
	mset(x,y,sprite+1)
end
		
function open_box(x,y)
	next_tile(x,y)
	p.box+=1
end
 
function pick_up_hearts(x,y)
 next_tile(x,y)
end
