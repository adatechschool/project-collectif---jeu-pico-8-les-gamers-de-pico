function addmob(typ,mx,my)
 m={
	x=mx,
	y=my,
	sprite=066,
	hp=3
}
	add(mob,m)
end

function draw_mob()
-- prise en compte cam
	local camx=flr(p.x/16)*16
	local camy=flr(p.y/16)*16
	camera(camx*8, camy*8)
	
	for m in all(mob) do
	spr(m.sprite,m.x*8,m.y*8)
	end
end
