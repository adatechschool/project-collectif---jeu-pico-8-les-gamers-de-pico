function _init()
	create_player()
	next_tile(x,y)
	t=0
-- pour creer les monstres 
		mob={}
		addmob(0,007,007) --type, x, y
		addmob(0,015,008)
		addmob(0,017,004)
		addmob(0,049,002)
		addmob(0,039,002)
		addmob(0,054,009)
		addmob(0,059,009)
		addmob(0,058,012)
		addmob(0,052,012)
		addmob(0,053,008)
		addmob(0,053,010)
		addmob(0,052,009)
		addmob(0,058,005)
	init_msg()
	inventory={}
	select = 0
	_draw = g_draw
	_update = g_update
	p.hearts = 3	
	
--variables
	scene="menu"
	x=63
 y=63

end

function g_update()
	update_camera()
	if not messages[1] then
		player_movement()
	end
	t+=1
	update_msg()

--main update and draw start
 if scene=="menu" then
  update_menu() 
 	elseif scene=="game" then
   update_game()
 end
end

function g_draw()
	cls()
-- changer la	couleur transparente par defaut
	palt(7,true)
	palt(0,false)
	draw_map()
	draw_player()	
	interact(x,y)
	draw_ui()
	draw_mob()
	draw_msg()
	
-- main menu game start
	if scene=="menu" then
  draw_menu()
 	elseif scene=="game" then
 end
end

--updates start
function update_menu()
 if btnp(❎) then
  scene="game"
 sfx(1) 
 end
end

function update_game()
 if btn(⬅️) then x-=1 end
 if btn(➡️) then x+=1 end
 if btn(⬆️) then y-=1 end
 if btn(⬇️) then y+=1 end 
end

--draws start
function draw_menu()
 cls()
 spr(131,10,20,13,5)
	print("press ❎ to start :)",
	22,90,3)    
end
