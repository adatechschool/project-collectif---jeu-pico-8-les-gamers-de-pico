-- message
function init_msg()
	messages={}
end

function create_msg(name,...)
	msg_title=name
	messages={...}
end

-- suppression msg avec ❎
function update_msg()
	if (btn(❎)) then
		deli(messages,1)
	end
end

-- apparence boite dialogue
function draw_msg()
	if messages[1] then
		local y=60
		camera()
		rectfill(8,y,6+#msg_title*5,y+13)
		print(msg_title,10,y+2,15)
		rectfill(4,y+9,125,y+23,15)
		rect(4,y+9,125,y+23,5)
		print(messages[1],6,y+11,5)
	end
end
